							kCCRelinkButtonWidgetID, kLinksUICCRelinkButtonIconRsrcID, kLinksUIPluginID,	// WidgetId, RsrcId
							kBindRight|kBindBottom,				// Frame binding
							Frame(kCC2016ResizablePanelBottomBottomBarWidth-(5*kCC2016PanelIconWidth),kCC2016StandardWidgetHeight+2+(8*kCC2016PanelTreeNodeHeight)+kSplitterBottomBarMargin,kCC2016ResizablePanelBottomBottomBarWidth-(4*kCC2016PanelIconWidth),kCC2016StandardWidgetHeight+2+(8*kCC2016PanelTreeNodeHeight) + kCC2016PanelIconHeight+kSplitterBottomBarMargin)		// Frame
							kTrue, kTrue,										// Visible, Enabled
							kADBEIconSuiteButtonType,
						),
						LinksUIButtonWidget
						(
							kRelinkButtonWidgetID, kLinksUIRelinkButtonIconRsrcID, kLinksUIPluginID,	// WidgetId, RsrcId
							kBindRight|kBindBottom,				// Frame binding
							Frame(kCC2016ResizablePanelBottomBottomBarWidth-(4*kCC2016PanelIconWidth),kCC2016StandardWidgetHeight+2+(8*kCC2016PanelTreeNodeHeight)+kSplitterBottomBarMargin,kCC2016ResizablePanelBottomBottomBarWidth-(3*kCC2016PanelIconWidth),kCC2016StandardWidgetHeight+2+(8*kCC2016PanelTreeNodeHeight) + kCC2016PanelIconHeight+kSplitterBottomBarMargin)		// Frame
							kTrue, kTrue,										// Visible, Enabled
							kADBEIconSuiteButtonType,
						),
						LinksUIButtonWidget
						(
							kGotoLinkButtonWidgetID, kLinksUIGotoLinkButtonIconResID, kLinksUIPluginID,	// WidgetId, RsrcId
							kBindRight|kBindBottom,				// Frame binding
							Frame(kCC2016ResizablePanelBottomBottomBarWidth-(3*kCC2016PanelIconWidth),kCC2016StandardWidgetHeight+2+(8*kCC2016PanelTreeNodeHeight)+kSplitterBottomBarMargin,kCC2016ResizablePanelBottomBottomBarWidth-(2*kCC2016PanelIconWidth),kCC2016StandardWidgetHeight+2+(8*kCC2016PanelTreeNodeHeight) + kCC2016PanelIconHeight+kSplitterBottomBarMargin)		// Frame
							kTrue, kTrue,										// Visible, Enabled
							kADBEIconSuiteButtonType,
						),
						LinksUIButtonWidget
