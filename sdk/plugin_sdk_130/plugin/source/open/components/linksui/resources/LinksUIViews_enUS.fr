							kLinkColumnSeparatorWidgetID,					// WidgetId
							0,0,				// RsrcId
							kBindLeft|kBindRight,						// Frame binding
							Frame(0, kCC2016StandardWidgetHeight+1, kCC2016PanelStandardWidth, kCC2016StandardWidgetHeight+2)		// Frame
