			kPageTransitionsPanelControlStripWidgetID, kPMRsrcID_None,
			kBindBottom | kBindLeft | kBindRight,
			Frame(kCC2016StandardGapBetweenWidgets,198,kCC2016PanelStandardWidth-kCC2016StandardGapBetweenWidgets,222)						// Frame
