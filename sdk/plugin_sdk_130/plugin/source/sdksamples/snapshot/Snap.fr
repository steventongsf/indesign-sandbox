/*
//    Boss class definition.
*/
resource ClassDescriptionTable(kSDKDefClassDescriptionTableResourceID)
{{{
            
            /**
            Provides the actions invoked by this plug - in's
            sub - menu items. 
            
            @ingroup snapshot
            */
            Class
            {
                kSnapActionComponentBoss,
                kBaseActionComponentBoss,
                {
                    /** 
                    Handle the actions from the menu. 
                    */
                    IID_IACTIONCOMPONENT, kSnapActionComponentImpl,
                }
            },
            
            /**
            Registers the tracker for this plug-in.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapTrackerRegisterBoss,
                kInvalidClass,
                {
                    /** 
                    Register the tracker with the application. 
                    */
                    IID_ITRACKERREGISTER, kSnapTrackerRegisterImpl,
                    /** 
                    Identify this boss as a tracker register service.
                    Implementation provided by the API.
                    */
                    IID_IK2SERVICEPROVIDER, kCTrackerRegisterProviderImpl,
                }
            },
            
            /** 
            Provides the tool behavior for this plug-in.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapToolBoss,
                kGenericToolBoss,
                {
                    /**
                    Provide the implementation for the tool's behavior.
                    */
                    IID_ITOOL, kSnapToolImpl,
                    /** 
                    Identify this boss as a cursor provider service.
                    Implementation provided by the API.
                    */
                    IID_ICURSORPROVIDER, kSnapCursorProviderImpl,
                    /** 
                    Persist the state of the tool across application instantiation.
                    Implementation provided by the API.
                    */
                    IID_IPMPERSIST, kPMPersistImpl,
                }
            },
            
            /** 
            Provides the tracker behavior for the tool in this plug-in.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapTrackerBoss,
                kInvalidClass,
                {
                    /** 
                    Provide the implementation for the tool's tracker.
                    */
                    IID_ITRACKER, kSnapTrackerImpl,
                }
            },
            
            /**
            Provides the snapshot preference dialog.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapDialogBoss,
                kDialogBoss,
                {
                    /** 
                    Provide the implementation for the dialog initialization and validation.
                    */
                    IID_IDIALOGCONTROLLER, kSnapDialogControllerImpl,
                }
            },
            
            /**
            Provides the command for setting the Snapshot tool's preference. 
            
            @ingroup snapshot
            */
            Class
            {
                kSnapSetPrefsCmdBoss,
                kInvalidClass,
                {
                    /** 
                    Provide the implementation for the command to set up the snapshot tool's 
                    preference. The prefs are saved only on the session workspace & the itemlist
                    is not used in the command. The command performs notification on kWorkspaceBoss
                    for IID_ISNAPPREFSDATA.
                    
                    */
                    IID_ICOMMAND, kSnapSetPrefsCmdImpl,
                    /** 
                    Provide the non-persistent implementation for ISnapPrefsData.
                    */
                    IID_ISNAPPREFSDATA, kSnapPrefsDataImpl,
                }
            },
            
            /**
            Registers the content iterator for the persistent implementation 
            of ISnapPrefsData interface.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapIteratorRegisterBoss,
                kInvalidClass,
                {
                    /** 
                    Register the content iterator for persistent implementation of ISnapPrefsData.
                    */
                    IID_ICONTENTITERATORREGISTER, kSnapIteratorRegisterImpl,
                    /** 
                    Implementation provided by the API.
                    */
                    IID_IK2SERVICEPROVIDER,  kContentIteratorRegisterServiceImpl,
                }
            },
            
            /**
            Adds one interface ISnapPrefsData to kWorkspaceBoss. The 
            implementation specifies how to get and set a set of preference value.
            
            @ingroup snapshot
            */
            AddIn
            {
                kWorkspaceBoss,
                kInvalidClass,
                {
                    /** 
                    Persistent implementation of ISnapPrefsData interface. 
                    */
                    IID_ISNAPPREFSDATA, kSnapPrefsDataPersistImpl
                }
            },
            
            /** Adds our custom selection suite interface to kIntegratorSuiteBoss.
            
            @ingroup snapshot
            
            */
            AddIn
            {
                kIntegratorSuiteBoss,
                kInvalidClass,
                {
                    /**    Suite interface. 
                    @see SnapSelectionSuiteASB
                    */
                    IID_ISNAPSELECTIONSUITE, kSnapSelectionSuiteASBImpl,
                }
            },
            
            /** Adds our custom selection suite interface to kLayoutSuiteBoss
            
            @ingroup snapshot
            */
            AddIn
            {
                kLayoutSuiteBoss,
                kInvalidClass,
                {
                    /** Suite interface.
                    @see SnapSelectionSuiteCSB
                    */
                    IID_ISNAPSELECTIONSUITE, kSnapSelectionSuiteCSBImpl,
                }
                
            },
            
            /** Provides error strings for the SnapShot plug-in.
            
            Added at InDesign CS2 to support scripting.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapErrorStringServiceBoss,
                kInvalidClass,
                {
                    /** See IErrorStringService / SnapErrorStringService.
                    */
                    IID_IERRORSTRINGSERVICE, kSnapErrorStringServiceImpl,        
                    /**    Identifies this boss as an error string provider.
                    */
                    IID_IK2SERVICEPROVIDER, kErrorStringProviderImpl,
                }
            },
            
            /** Handles scripting of SnapShot preferences
            in interface ISnapPrefsData on the session workspaces.
            
            Added at InDesign CS2 to support scripting.
            
            @ingroup snapshot
            */
            Class
            {
                kSnapPrefsScriptProviderBoss,
                kBaseScriptProviderBoss,
                {
                    /** See IScriptProvider / SnapPrefsScriptProvider.
                    */
                    IID_ISCRIPTPROVIDER, kSnapPrefsScriptProviderImpl,    
                }
            },
            
            /** This boss class supplies a schema-based conversion service for this plug-in.
            @ingroup snapshot
            */
            Class
            {
                kSnapConversionProviderBoss,
                kInvalidClass,
                {
                    /** Provides the conversion service.
                    Implementation provided by the API.
                    */
                    IID_ICONVERSIONPROVIDER, kSchemaBasedConversionImpl,
                    /** Registers this boss as a conversion service provider.
                    Implementation provided by the API.
                    */
                    IID_IK2SERVICEPROVIDER, kConversionServiceImpl,
                }
            },
        }}};

