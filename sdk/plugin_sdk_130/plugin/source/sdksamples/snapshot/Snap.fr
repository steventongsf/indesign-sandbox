/*
// Scripting definition.
*/
resource VersionedScriptElementInfo(1)
{
    //Contexts
    {
        // Scripting support added at InDesign CS 2.0.
        kCS2ScriptVersion, kCoreScriptManagerBoss, kWildFS, k_Wild,
    }
    
    //Elements
    {
        
        // Defines new script element object for this plug-in's preferences
        Object
        {
            kSnapPrefObjectScriptElement,    // this object's script element ID
            c_SnapPref,                      // ScriptID
            "snapshot preference",           // name
            "SnapShot preference",           // description
            kSnapPref_CLSID,                 // Windows CLSID
            NoCollectionInfo,                // CollectionElementType (scriptID, name, descr, CLSID)
            kPreferencesObjectScriptElement, // base object script element ID
            kPreferencesSuiteScriptElement,  // suite script element ID
        }
        
        // Allows the object above to be accessed by clients as a property
        Property
        {
            kSnapPrefObjectPropertyScriptElement,
            p_SnapPref,
            "snapshot preferences",
            "SnapShot preferences",
            ObjectType(kSnapPrefObjectScriptElement ),
            {}
            kNoAttributeClass,
        }    
        
        // file name for your snap shot - a string label
        Property
        {
            kSnapFileNamePropertyScriptElement,        // this property's script element ID
            p_SnapFileName,                            // ScriptID
            "snapshot file name",                      // name
            "File name to which you want your snap shot saved",    // description
            StringType,                                // type
            {}                                         // alternate types
            kNoAttributeClass,                         // attribute class ID
        }
        
        // enum for image write format
        Enum
        {
            kSnapImageWriteFormatEnumScriptElement,
            en_SnapImgeWriteFormat,
            "snapshot image write format enum",
            "Available SnapShot image output format",
            {
                en_SnapExPNG, "snapshot PNG", "Portable Network Graphics",
                en_SnapExGIF, "snapshot GIF", "CompuServe GIF",
                en_SnapExSCT, "snapshot SCT", "SciTexCT",
                en_SnapExTIFF, "snapshot TIFF", "TIFF",
                en_SnapExJPEG, "snapshot JPEG", "JPEG",
            },
        }
        
        // image write format - an enum declared in kSnapImageWriteFormatEnumScriptElement
        Property
        {
            kSnapImageFormatPropertyScriptElement,        // this property's script element ID
            p_SnapImageFormat,                            // ScriptID
            "snapshot image write format",                // name
            "Image write format class ID",                // description
            EnumType(kSnapImageWriteFormatEnumScriptElement),   // type
            {}                                            // alternate types
            kNoAttributeClass,                            // attribute class ID
        }
        
        // snap shot x and y scale factor, a real
        Property
        {
            kSnapScaleFactorPropertyScriptElement,  // this property's script element ID
            p_SnapScale,                            // ScriptID
            "snapshot scale factor",                // name
            "Snap shot x and y scale factor",       // description
            RealType,                               // type
            {}                                      // alternate types
            kNoAttributeClass,                      // attribute class ID
        }
        
        // snap shot image resolution, a real
        Property
        {
            kSnapResolutionPropertyScriptElement,   // this property's script element ID
            p_SnapResolution,                       // ScriptID
            "snapshot image resolution",            // name
            "Snap shot image resolution",           // description
            RealType,                               // type
            {}                                      // alternate types
            kNoAttributeClass,                      // attribute class ID
        }
        
        // snap shot minimum image resolution, a real
        Property
        {
            kSnapMinimumResolutionPropertyScriptElement, // this property's script element ID
            p_SnapMinimumResolution,                     // ScriptID
            "snapshot minimum image resolution",         // name
            "Snap shot minimum image resolution",        // description
            RealType,                                    // type
            {}                                           // alternate types
            kNoAttributeClass,                           // attribute class ID
        }
        
        // snap shot bleed, a real based on unit of measurement
        Property
        {
            kSnapBleedPropertyScriptElement,        // this property's script element ID
            p_SnapBleed,                            // ScriptID
            "snapshot bleed",                       // name
            "Snap shot bleed",                      // description
            UnitMinMaxType(-1440, 1440),            // type
            {}                                      // alternate types
            kNoAttributeClass,                      // attribute class ID
        }
        
        // snap shot's draw area, a bool that returns kTrue if draw the clicked spread, returns kFalse if draw the selected items.
        Property
        {
            kSnapDrawAreaPropertyScriptElement,        // this property's script element ID
            p_SnapDrawArea,                            // ScriptID
            "snapshot draw area",                      // name
            "Snap shot the whole clicked spread or selected items",    // description
            BoolType,                                  // type
            {}                                         // alternate types
            kNoAttributeClass,                         // attribute class ID
        }
        
        // snap shot's graphic resolution, a bool that returns kTrue if the image is drawn at full resolution, otherwise returns kFalse.
        Property
        {
            kSnapFullResolutionPropertyScriptElement,  // this property's script element ID
            p_SnapFullResolutionGraphics,              // ScriptID
            "snapshot full resolution graphics",       // name
            "Snap shot graphic drawn in full resolution or proxy",    // description
            BoolType,                                  // type
            {}                                         // alternate types
            kNoAttributeClass,                         // attribute class ID
        }
        
        // snap shot's color mode, a bool that returns kTrue if image is draw in grayscale, otherwise returns kFalse.
        Property
        {
            kSnapDrawGrayPropertyScriptElement,        // this property's script element ID
            p_SnapDrawGray,                            // ScriptID
            "snapshot draw gray",                      // name
            "Snap shot drawn in grayscale or rdb",     // description
            BoolType,                                  // type
            {}                                         // alternate types
            kNoAttributeClass,                         // attribute class ID
        }
        
        // snap shot drawing flags.
        Property
        {
            kSnapDrawingFlagsPropertyScriptElement,    // this property's script element ID
            p_SnapDrawingFlags,                        // ScriptID
            "snapshot drawing flags",                  // name
            "Snap shot's drawing flags",               // description
            Int32Type,                                 // type
            {}                                         // alternate types
            kNoAttributeClass,                         // attribute class ID
        }
        
        // snap shot JPEG encoding.
        Property
        {
            kSnapJPEGEncodingPropertyScriptElement,    // this property's script element ID
            p_SnapJPEGEncoding,                        // ScriptID
            "snapshot JPEG encoding",                  // name
            "Snap shot's JPEG encoding",               // description
            Int32Type,                                 // type
            {}                                         // alternate types
            kNoAttributeClass,                         // attribute class ID
        }
        
        // snap shot JPEG quality.
        Property
        {
            kSnapJPEGQualityPropertyScriptElement,    // this property's script element ID
            p_SnapJPEGQuality,                        // ScriptID
            "snapshot JPEG quality",                  // name
            "Snap shot's JPEG quality",               // description
            Int32Type,                                // type
            {}                                        // alternate types
            kNoAttributeClass,                        // attribute class ID
        }
        
        // snap shot palette type for TIFF image.
        Property
        {
            kSnapTIFFPaletteTypePropertyScriptElement,// this property's script element ID
            p_SnapTIFFPaletteType,                    // ScriptID
            "snapshot TIFF palette type",             // name
            "Snap shot's TIFF Palette Type",          // description
            Int32Type,                                // type
            {}                                        // alternate types
            kNoAttributeClass,                        // attribute class ID
        }
        
        // snap shot palette type for GIF image.
        Property
        {
            kSnapGIFPaletteTypePropertyScriptElement, // this property's script element ID
            p_SnapGIFPaletteType,                     // ScriptID
            "snapshot GIF palette type",              // name
            "Snap shot's GIF Palette Type",           // description
            Int32Type,                                // type
            {}                                        // alternate types
            kNoAttributeClass,                        // attribute class ID
        }
        
        // snap shot transparency flag for TIFF image.
        Property
        {
            kSnapTIFFTransparentPropertyScriptElement,// this property's script element ID
            p_SnapTIFFTransparent,                    // ScriptID
            "snapshot TIFF transparency",             // name
            "Snap shot's TIFF Transparency",          // description
            BoolType,                                 // type
            {}                                        // alternate types
            kNoAttributeClass,                        // attribute class ID
        }
        
        // snap shot transparency flag for GIF image.
        Property
        {
            kSnapGIFTransparentPropertyScriptElement, // this property's script element ID
            p_SnapGIFTransparent,                     // ScriptID
            "snapshot GIF transparency",              // name
            "Snap shot's GIF Transparency",           // description
            BoolType,                                 // type
            {}                                        // alternate types
            kNoAttributeClass,                        // attribute class ID
        }
        
        // snap shot interlace flag for GIF image.
        Property
        {
            kSnapGIFInterlacedPropertyScriptElement,  // this property's script element ID
            p_SnapGIFInterlaced,                      // ScriptID
            "snapshot interlace flag",                // name
            "Snap shot's interlace flag",             // description
            BoolType,                                 // type
            {}                                        // alternate types
            kNoAttributeClass,                        // attribute class ID
        }
        
        // Connects this plug-in's preferences to scripting.
        Provider
        {
            kSnapPrefsScriptProviderBoss,
            {
                // Parent the SnapShot preferences on the application:
                Parent{kApplicationObjectScriptElement },    
                
                // The SnapShot object:
                RepresentObject{kSnapPrefObjectScriptElement },
                
                // The property on the parent object returns the SnapShot preferences object:
                ParentProperty{kSnapPrefObjectPropertyScriptElement, kReadOnly },    
                
                // The preferences:
                Property{kSnapFileNamePropertyScriptElement, kReadWrite },                            
                Property{kSnapImageFormatPropertyScriptElement, kReadWrite },                            
                Property{kSnapScaleFactorPropertyScriptElement, kReadWrite },                            
                Property{kSnapResolutionPropertyScriptElement, kReadWrite },                            
                Property{kSnapMinimumResolutionPropertyScriptElement, kReadWrite },                            
                Property{kSnapBleedPropertyScriptElement, kReadWrite },                            
                Property{kSnapDrawAreaPropertyScriptElement, kReadWrite },                            
                Property{kSnapFullResolutionPropertyScriptElement, kReadWrite },                            
                Property{kSnapDrawGrayPropertyScriptElement, kReadWrite },                            
                Property{kSnapDrawingFlagsPropertyScriptElement, kReadWrite },                            
                Property{kSnapJPEGEncodingPropertyScriptElement, kReadWrite },                            
                Property{kSnapJPEGQualityPropertyScriptElement, kReadWrite },                            
                Property{kSnapTIFFPaletteTypePropertyScriptElement, kReadWrite },                            
                Property{kSnapGIFPaletteTypePropertyScriptElement, kReadWrite },                            
                Property{kSnapTIFFTransparentPropertyScriptElement, kReadWrite },                            
                Property{kSnapGIFTransparentPropertyScriptElement, kReadWrite },                            
                Property{kSnapGIFInterlacedPropertyScriptElement, kReadWrite },                            
            }
        }
    }
};
